{
    "name": "storefront_backend",
    "version": "0.1.0",
    "description": "## Getting Started",
    "main": "server.ts",
    "scripts": {
        "start": "nodemon src/server.ts",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
        "jasmine": "SET ENV=test&& db-migrate up --config database.json -e test && jasmine && db-migrate reset --config database.json -e test",
        "build": "npx tsc",
        "test": "npm run build && npm run jasmine",
        "lint": "eslint . --ext .ts",
        "format": "prettier --config .prettierrc **/*.* --write"
    },
    "author": "Ahmed Abdelaziz",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.7.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jasmine": "^4.0.3",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.35",
        "@types/pg": "^8.6.5",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^4.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.16",
        "supertest": "^6.2.3",
        "ts-node": "^8.10.2",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.7.2"
    }
}
